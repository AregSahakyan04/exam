printf(" Input the second number : ");
scanf("%d", &sno);
= &fno;
= &sno;

 
  sum =
 
*ptr +
 
*qtr;
 

printf(" The sum of the entered numbers is : %d\n\n",sum);

return 0;
}
//47  Find the maximum number between two numbers	
#include <stdio.h>
 
void
{
 
main()
 
int fno,sno,*ptr1=&fno,*ptr2=&sno;
